
class Node:
    def __init__(self,value):
        self.data = value
        self.next = None
        
class Linked:
    def __init__(self):
        self.head = None
        
    def Show(self):
        node = self.head
        while node is not None:
            print(node.data)
            node = node.next
            
link = Linked()
elem1 = Node("FirstNode")
link.head = elem1


elem2 = Node("SecondNode")
link.head.next = elem2
link.Show()
            
